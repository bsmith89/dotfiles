Include config_local

IdentityFile ~/.ssh/id_rsa

host itd-login
    HostName login.itd.umich.edu
    User bjsm
    ForwardX11 yes
    ForwardX11Trusted yes

host github
    HostName github.com
    IdentitiesOnly yes
    User git

host flux-*
    IdentitiesOnly yes
    User bjsm
    ForwardX11 yes
    ForwardX11Trusted yes

host flux-auto
    HostName flux-login.arc-ts.umich.edu

host flux-1
    HostName flux-login1.arc-ts.umich.edu

host flux-2
    HostName flux-login2.arc-ts.umich.edu

host flux-3
    HostName flux-login3.arc-ts.umich.edu

host flux-f
    HostName flux-xfer.arc-ts.umich.edu
    ForwardX11 no

host webhost
    HostName byronjsmith.com
    IdentitiesOnly yes
    User byronjsm

host hpcc
    HostName hpcc.msu.edu
    IdentitiesOnly yes
    User smithby1

host pollard-*
    IdentitiesOnly yes
    User bsmith

host pollard-chestnut
    HostName chestnut

host pollard-bueno
    HostName bueno

host pollard-westway
    HostName westway

host pollard-ruby
    HostName ruby

host wynton-*
    User bsmith
    PreferredAuthentications=publickey,keyboard-interactive
    IdentitiesOnly yes

host wynton-login1
    HostName log2.wynton.ucsf.edu

host wynton-login2
    HostName log2.wynton.ucsf.edu

host wynton-f
    HostName dt1.wynton.ucsf.edu

host wynton-dev1
    HostName dev1
    ProxyCommand ssh %r@wynton-login2 -W %h:%p

host wynton-dev2
    HostName dev2
    ProxyCommand ssh %r@wynton-login2 -W %h:%p

host wynton-dev3
    HostName dev3
    ProxyCommand ssh %r@wynton-login2 -W %h:%p

host wynton-gpudev1
    HostName gpudev1
    ProxyCommand ssh %r@wynton-login2 -W %h:%p

ControlMaster auto
ControlPath ~/.ssh/control:%h:%p:%r

# To use ScaleFT proxycommand, add this configuration block to your $HOME/.ssh/config
Match exec "/usr/local/bin/sft resolve -q  %h 2>/dev/null"
    ProxyCommand "/usr/local/bin/sft" proxycommand  %h
    UserKnownHostsFile "/Users/byronsmith/Library/Application Support/ScaleFT/proxycommand_known_hosts"
